generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id        String    @id
  fullName  String
  email     String    @unique(map: "User_email_key")
  password  String
  createdAt DateTime  @default(now())
  updatedAt DateTime
  role      user_role @default(user)
}

enum user_role {
  admin
  user
}
